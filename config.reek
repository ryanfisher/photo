Attribute:
  exclude:
    - PhotoProcessor#photo

DuplicateMethodCall:
  exclude:
    - Album#update_with

IrresponsibleModule:
  enabled: false

TooManyInstanceVariables:
  exclude:
    - PhotoProcessor    

